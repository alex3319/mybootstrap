extends templates/layout.pug
block content

  header

    .container-fluid.row.bg-success
      .container
        .row
          .col-md-2.col-sm-2.col-xs-12
            article
              img.img-responsive.center-block(src="assets/img/tablichka.png")
          .col-md-9.col-sm-9.col-xs-12
            article
              h1.text-center.white Ейский историко-краеведческий музей
          .col-md-1.col-sm-1.hidden-xs
            article
              img.img-responsive.center-block(src="assets/img/eye.gif")

    nav.navbar.navbar-inverse
      .container
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') Bootstrap theme
        #navbar.navbar-collapse.collapse
          ul.nav.navbar-nav
            li.active
              a(href='#') О музее
            li.active
              a(href='#') Деятельность
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                | Выставки
                span.caret
              ul.dropdown-menu
                li
                  a(href='#') Постоянные
                li
                  a(href='#') Временные
                li
                  a(href='#') Архив
                li.divider(role='separator')
                li.dropdown-header Лекторий
                li
                  a(href='#') Культурные программы
                li
                  a(href='#') Фестивали
                li
                  a(href='#') Концерты
        // /.nav-collapse

  .container
    #carousel-example-generic.carousel.slide(data-ride='carousel')
      ol.carousel-indicators
        li(data-target='#carousel-example-generic', data-slide-to='0')
        li.active(data-target='#carousel-example-generic', data-slide-to='1')
        li(data-target='#carousel-example-generic', data-slide-to='2')
      .carousel-inner(role='listbox')
        .item
          img(data-src='holder.js/1140x500/auto/#777:#555/text:First slide', alt='First slide [1140x500]', src='data:image/svg+xml;base64,', data-holder-rendered='true')
        .item.active
          img(data-src='holder.js/1140x500/auto/#666:#444/text:Second slide', alt='Second slide [1140x500]', src='data:image/svg+xml;base64,', data-holder-rendered='true')
        .item
          img(data-src='holder.js/1140x500/auto/#555:#333/text:Third slide', alt='Third slide [1140x500]', src='data:image/svg+xml;base64,', data-holder-rendered='true')
      a.left.carousel-control(href='#carousel-example-generic', role='button', data-slide='prev')
        span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
        span.sr-only Previous
      a.right.carousel-control(href='#carousel-example-generic', role='button', data-slide='next')
        span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
        span.sr-only Next






  hr
  footer
    p © 2017 Company, Inc.



  // /.container

